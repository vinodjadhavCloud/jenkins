pipeline {
    agent any
    stages {
        stage("scm checkout") {
            steps {
                sh 'echo "hello"'
                }

        }

        stage('code validate')
        {
            steps {
                sleep 10
                }
            }
        }
        stage('code compile')
        {
            steps {
                     sh 'echo "hesssssssssllo"'

                }
            }
        }